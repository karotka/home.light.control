/www/sklik/importweb/log/*_log {
	daily
	compress
	delaycompress
        dateext
	rotate 60
	missingok
	nocreate
	sharedscripts
    prerotate
        find /www/sklik/importweb/tmp/* -mmin +120 -exec rm {} \;
        find /www/sklik/importweb/savedfiles/* -mtime +14 -exec rm {} \;
	postrotate
        /etc/init.d/szn-sklik-importweb reload
	endscript
}
